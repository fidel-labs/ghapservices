            ".card-wrapper when (@showBorders) and (@showTeam) and (@showPadding)": {
              "border": "1px solid @bordersColor"
            },
            ".card-subtitle:before when (@lineSpecialty) and (@showTeam)": {
              "content": "''",
              "width": "25px",
              "height": "3px",
              "display": "inline-block",
              "vertical-align": "middle",
              "margin-right": "10px",
              "background-color": "@primaryColor"
            },
            "@media (max-width: 767px)": {
              ".card:not(.last-child) when (@showTeam)": {
                "padding-bottom": "20px"
              },
              ".mbr-jc-e": {
                "justify-content": "center"
              }
