              }
            },
            ".form-container, .first-element, .first-block": {
              "width": "100%"
            },
            ".form-container, .form-wrap, .form-box, .form-block": {
              "position": "relative"
            },
            ".mbr-section-btn": {
              "padding-bottom": "0"
